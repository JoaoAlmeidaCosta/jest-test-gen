{
  "name": "jest-test-gen",
  "version": "1.0.1",
  "description": "Generator of jest unit test with all imports mocked and tests stub for every class and function exported",
  "keywords": [
    "jest",
    "unit test generator",
    "mocks",
    "testing",
    "jest generator",
    "es6 classes",
    "functions",
    "export parsing"
  ],
  "main": "lib/public-api.js",
  "scripts": {
    "build:dev": "tsc --watch",
    "build": "tsc",
    "dev": "ts-node ./src/cli.ts",
    "test": "jasmine --require=ts-node/register"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:egm0121/jest-test-gen.git"
  },
  "author": "egm0121",
  "license": "MIT",
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/jasmine": "^2.8.7",
    "@types/lodash": "^4.14.141",
    "@types/node": "^11.11.6",
    "jasmine": "^3.5.0",
    "ts-node": "^10.3.0"
  },
  "bin": {
    "jest-test-gen": "./lib/cli.js"
  },
  "dependencies": {
    "compare-versions": "^4.1.3",
    "debug": "4.3.2",
    "latest-version": "^5.1.0",
    "lodash": "^4.1.*",
    "typescript": "^4.4.2"
  },
  "peerDependencies": {
    "typescript": "^4.4.2"
  }
}
